<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical">

    <include layout="@layout/toolbar_layout"/>

    <GridLayout
        android:id="@+id/gridLayout"
        android:layout_width="411dp"
        android:layout_height="732dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="20dp"
        android:columnCount="4"
        android:rowCount="3">

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="0"
            android:layout_column="0"
            app:srcCompat="@drawable/fullstop" />

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="0"
            android:layout_column="1"
            app:srcCompat="@drawable/semicolon" />

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="0"
            android:layout_column="2"
            app:srcCompat="@drawable/exclamation" />

        <ImageView
            android:id="@+id/imageView23"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="0"
            android:layout_column="3"
            app:srcCompat="@drawable/questionmark" />

        <ImageView
            android:id="@+id/imageView24"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="1"
            android:layout_column="0"
            app:srcCompat="@drawable/dash" />

        <ImageView
            android:id="@+id/imageView25"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="1"
            android:layout_column="1"
            app:srcCompat="@drawable/hash" />

        <ImageView
            android:id="@+id/imageView26"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="1"
            android:layout_column="2"
            app:srcCompat="@drawable/apostrophe" />

        <ImageView
            android:id="@+id/imageView27"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="1"
            android:layout_column="3"
            app:srcCompat="@drawable/attherate" />

        <ImageView
            android:id="@+id/imageView28"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="2"
            android:layout_column="0"
            app:srcCompat="@drawable/equalto" />

        <ImageView
            android:id="@+id/imageView29"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="2"
            android:layout_column="1"
            app:srcCompat="@drawable/lessthan" />

        <ImageView
            android:id="@+id/imageView30"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="2"
            android:layout_column="2"
            app:srcCompat="@drawable/attherate" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="0"
            android:layout_column="0"
            android:text="."
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="0"
            android:layout_column="1"
            android:text=";"
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="0"
            android:layout_column="2"
            android:text="!"
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="0"
            android:layout_column="3"
            android:text="@string/questionmark"
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="1"
            android:layout_column="0"
            android:text="-"
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView36"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="1"
            android:layout_column="1"
            android:text="#"
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView37"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="1"
            android:layout_column="2"
            android:text="'"
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView38"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="1"
            android:layout_column="3"
            android:text=" @"
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView39"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="2"
            android:layout_column="0"
            android:text="="
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView40"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="2"
            android:layout_column="1"
            android:text="@string/lessthan"
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold"
            android:visibility="visible" />
        <TextView
            android:id="@+id/textView41"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="2"
            android:layout_column="2"
            android:text="@string/greaterthan"
            android:textAlignment="center"
            android:textSize="25sp"
            android:textStyle="bold"
            android:visibility="visible" />
    </GridLayout>
</RelativeLayout>