<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Numberactivity"
    android:orientation="vertical">

    <include layout="@layout/toolbar_layout"/>

    <GridLayout
        android:id="@+id/gridLayout"
        android:layout_width="411dp"
        android:layout_height="732dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="20dp"
        android:columnCount="4"
        android:rowCount="3">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="0"
            android:layout_column="0"
            app:srcCompat="@drawable/j_alphabet" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="0"
            android:layout_column="1"
            app:srcCompat="@drawable/a_alphabet" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="0"
            android:layout_column="2"
            app:srcCompat="@drawable/b_alphabet" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="0"
            android:layout_column="3"
            app:srcCompat="@drawable/c_alphabet" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="1"
            android:layout_column="0"
            app:srcCompat="@drawable/d_alphabet" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="1"
            android:layout_column="1"
            app:srcCompat="@drawable/e_alphabet" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="1"
            android:layout_column="2"
            app:srcCompat="@drawable/f_alphabet" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="1"
            android:layout_column="3"
            app:srcCompat="@drawable/g_alphabet" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="2"
            android:layout_column="0"
            app:srcCompat="@drawable/h_alphabet" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="90dp"
            android:layout_height="130dp"
            android:layout_row="2"
            android:layout_column="1"
            app:srcCompat="@drawable/i_alphabet" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="2"
            app:srcCompat="@drawable/number_icon" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="91dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="3"
            app:srcCompat="@drawable/dis_icon" />

        <TextView
            android:id="@+id/textView55"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="2"
            android:text="Number Sign"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView56"
            android:layout_width="86dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="3"
            android:text="Decimal"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="0"
            android:layout_column="0"
            android:text="0"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="0"
            android:layout_column="1"
            android:text="1"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            android:visibility="visible" />
        <TextView
            android:id="@+id/textView5"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="0"
            android:layout_column="2"
            android:text="2"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="0"
            android:layout_column="3"
            android:text="3"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="1"
            android:layout_column="0"
            android:text="4"
            android:textAlignment="center"
            android:textSize="18sp"
            android:visibility="visible"
            tools:ignore="DuplicateIds" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="1"
            android:layout_column="1"
            android:text="5"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="1"
            android:layout_column="2"
            android:text="6"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            android:visibility="visible"
            tools:ignore="DuplicateIds" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="1"
            android:layout_column="3"
            android:text="7"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="2"
            android:layout_column="0"
            android:text="8"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="90dp"
            android:layout_height="29dp"
            android:layout_row="2"
            android:layout_column="1"
            android:text="9"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            android:visibility="visible" />

    </GridLayout>
</RelativeLayout>